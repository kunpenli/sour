//点击查询出来的数据,如果搜索框里有数据,就根据这个跳转；
$('.sear_resu').on('click','a',function(){
	var pinzhong=$('.pinzhongcode b').text(),
		heyue=$('.heyuename').text(),
		userid='${sessionScope.name}',
		obj,
		sear_txt=$('.big_sear').val();
		
		if( sear_txt==''){
			obj=decodeURI ( location.href.split('=')[1] );
		}else{
			obj=sear_txt;
		}
	//判断是否登录，未登录请先登录；
	if( !userid ){
		 layer.msg('提示:请先去登录!', {
	        time: 2000, //20s后自动关闭
	        btn: ['去登录','放弃'],
	        success:function( layero ){
	        	var btn = layero.find('.layui-layer-btn');
	            btn.find('.layui-layer-btn0').attr({
	              href: '<%=basePath%>user/tologin.do'
	              ,target: '_self'
	            });
	        }
	      }); 
	}else{
		sessionStorage.setItem('seardata',pinzhong+'-'+heyue+'-'+userid+'-'+obj );//存储关键
		window.open('<%=basePath%>page/marketguss.jsp?lisid=1','_self');
    }
});
//点击sear_btn  按钮;
$('body').on("click",".sear_btn",function(){
	var sear_txt=$('.big_sear').val(); ///搜索框input;
	window.location.href=location.origin+location.pathname+'?sear='+sear_txt;
	renfn( sear_txt );
});
function renfn( obj ){
	$('.big_sear').val( obj );	
	var sear_bool=false;
	if( obj=='' ){
		layer.msg('提示:请输入搜索内容!', {
	  	 time: 2000
	 	}); 
	}else{
		$('.sear_resu,.sear_wendang').html('');  //点击搜索，清空resu和wendang这两个div内容；
		$.ajax({						//查询后台
		 	url:'<%=basePath%>common/getInstruProduct.do',
			type:'post',
			data:{'sear':obj},
			async:false,
			success:function(data){
				if( !data ){
					sear_bool=false;//没有匹配结果再进行二次判断；
				}else{
					sear_bool=true;
					da_da=JSON.parse( data );
					ren_list( da_da);
				}
			}
		});        						
		///查询在线帮助文档
		var sear_count=highlight( obj );
		if( sear_count==0 ){
			sear_bool=false;
		}else{	//搜索文档数量大于0
			sear_bool=true;
		}
		if( !sear_bool){
			$('.sear_resu').text( "对不起，没有找到匹配结果."); 
		}
	}
}
///进行渲染;
function ren_list(da_da ){
	var str='',pinzh=[],heyue=[],titstr='',heyuestr='';
	
	for(var i=0;i<da_da.length-1;i+=2){
		if( pinzh.indexOf( da_da[i+1] )==-1){
			pinzh.push( da_da[i+1] );
		}
		heyue.push( da_da[i] );
	}
	//拼接品种代码;
	for(var i=0,num=pinzh.length;i<num;i++){
		if(i==num-1){
			titstr+=pinzh[i]
		}else{
			titstr+=pinzh[i]+','
		}
	}
	for(var j in heyue){
		if(j==heyue.length-1){
			heyuestr+=heyue[j]
		}else{
			heyuestr+=heyue[j]+','
		}
	}
	str+='<a href="javascript:;">'+
	 '<span class="pinzhongcode"><b>'+ titstr+'</b>   品种合约价格监控</span>'+
	 '<br/>'+
	 '<span class="heyuename">'+ heyuestr +'</span>'+
	 '</a>'
	$('.sear_resu').html( str );
}
</script>
