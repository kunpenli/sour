.boxy-wrapper { position: absolute; }
.boxy-wrapper.fixed { position: fixed; }
.boxy-modal-blackout { position: absolute; background-color: black; left: 0px; top: 0px; }
.boxy-wrapper { empty-cells: show; }
.boxy-wrapper .top-left, .boxy-wrapper .top-right, .boxy-wrapper .bottom-right, .boxy-wrapper .bottom-left { width: 10px; height: 10px; padding: 0px; }
.boxy-wrapper .top-left { background: transparent url("../images/boxy-nw.png") repeat scroll 0% 0%; }
.boxy-wrapper .top-right { background: transparent url("..images/s_topright.png") repeat scroll 0% 0%; }
.boxy-wrapper .bottom-right { background: transparent url("../images/s_btmright.png") repeat scroll 0% 0%; }
.boxy-wrapper .bottom-left { background: transparent url("../images/s_btmleft.png") repeat scroll 0% 0%; }
.boxy-wrapper .top-left {  }
.boxy-wrapper .top-right {  }
.boxy-wrapper .bottom-right {  }
.boxy-wrapper .bottom-left {  }
.boxy-wrapper .top { background: transparent url("../images/s_top.png") repeat scroll 0% 0%; padding: 0px; }
.boxy-wrapper .bottom { background: transparent url("../images/s_btm.png") repeat scroll 0% 0%; padding: 0px; }
.boxy-wrapper .left { background: transparent url("../images/s_left.png") repeat scroll 0% 0%; padding: 0px; }
.boxy-wrapper .right { background: transparent url("../images/s_right.png") repeat scroll 0% 0%; padding: 0px; }
.boxy-wrapper .title-bar { position: relative; background: transparent url("point2.gif") no-repeat scroll 5px center; height: 25px; padding-left: 16px; }
.boxy-wrapper .title-bar.dragging { cursor: move; }
.boxy-wrapper .title-bar h2 { font-size: 12px; color: rgb(0, 0, 0); line-height: 1; margin: 0px; padding: 5px 0px 0px; font-weight: bold; }
.boxy-wrapper .title-bar .close { color: white; z-index: 1104; right: 2px; cursor: pointer; position: absolute; top: 0px; font-size: 12px; }
.boxy-wrapper .title-bar .close:hover {  }
.boxy-inner { background-color: white; padding: 0px; }
.boxy-content { padding: 2px; }
.boxy-wrapper .question { width: 350px; min-height: 60px; }
.boxy-wrapper .answers { text-align: right; }
.boxy-btn1, .boxy-btn2, .boxy-btn3 { padding: 3px 12px 1.9px; border-width: 1px; border-style: solid; letter-spacing: 1px; line-height: 15px; cursor: pointer; overflow: visible; }
.boxy-btn1 { background-color: rgb(66, 95, 153); border-color: rgb(90, 126, 198) rgb(44, 62, 95) rgb(44, 62, 95) rgb(90, 126, 198); color: white; }
.boxy-btn2 { background-color: rgb(236, 233, 216); border-color: rgb(238, 238, 238) rgb(187, 187, 187) rgb(187, 187, 187) rgb(238, 238, 238); color: rgb(51, 51, 51); }
.boxy-btn3 { background-color: rgb(234, 234, 234); border-color: rgb(234, 234, 234) rgb(170, 170, 170) rgb(170, 170, 170) rgb(234, 234, 234); color: rgb(51, 51, 51); padding: 3px 5px 1.9px; }
.boxy-btn3:hover { background-color: rgb(202, 213, 235); border-color: rgb(206, 216, 227) rgb(162, 173, 188) rgb(162, 173, 188) rgb(206, 216, 227); }
